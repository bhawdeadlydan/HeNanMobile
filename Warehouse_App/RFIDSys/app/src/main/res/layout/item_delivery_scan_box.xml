<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="40dp"
        android:paddingTop="5dp"
        android:text="（ERP/套）编码："
        android:id="@+id/text_delivery_scan_mat_code"
        android:layout_alignParentTop="true"/>

    <TextView
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:paddingLeft="40dp"
        android:paddingBottom="5dp"
        android:text="应出数量："
        android:id="@+id/text_delivery_scan_expected_count"
        android:layout_below="@+id/text_delivery_scan_mat_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="物料名称："
        android:paddingLeft="40dp"
        android:id="@+id/text_delivery_scan_mat_name"
        android:layout_below="@+id/text_delivery_scan_mat_code"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
