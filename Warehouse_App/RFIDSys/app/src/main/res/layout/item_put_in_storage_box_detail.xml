<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="（ERP/套）编码："
        android:id="@+id/text_put_in_storage_box_detailmat_code"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="数量："
        android:id="@+id/text_put_in_storage_box_detail_count"
        android:layout_below="@+id/text_put_in_storage_box_detailmat_code"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="计量单位："
        android:id="@+id/text_put_in_storage_box_detail_unit"
        android:layout_marginEnd="40dp"
        android:layout_below="@+id/text_put_in_storage_box_detailmat_code"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
