<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/linearLayout8">

        <TextView
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:text="是否套设备："
            android:id="@+id/text_receiving_scan_box_detail_is_bom" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout8"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/linearLayout9">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="物料名称："
            android:id="@+id/text_receiving_scan_box_detail_item_name"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout9"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/linearLayout14">

        <TextView
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:text="货物数量："
            android:id="@+id/text_receiving_scan_box_detail_quantity" />

        <TextView
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:text="计量单位："
            android:id="@+id/text_receiving_scan_box_detail_unit" />
    </LinearLayout>

</RelativeLayout>
