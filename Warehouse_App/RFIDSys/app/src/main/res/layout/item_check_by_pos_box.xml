<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:paddingTop="5dp"
        android:text="已扫到数量："
        android:id="@+id/text_check_by_pos_real_count"
        android:layout_below="@+id/text_check_by_pos_mat_code"
        android:layout_toRightOf="@+id/text_check_by_pos_expected_count"
        android:layout_toEndOf="@+id/text_check_by_pos_expected_count" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="40dp"
        android:layout_marginTop="5dp"
        android:text="（ERP/套）编码："
        android:id="@+id/text_check_by_pos_mat_code"
        android:layout_alignParentTop="true" />

    <TextView
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:paddingLeft="40dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="应有数量："
        android:id="@+id/text_check_by_pos_expected_count"
        android:layout_below="@+id/text_check_by_pos_mat_code"/>
</RelativeLayout>
