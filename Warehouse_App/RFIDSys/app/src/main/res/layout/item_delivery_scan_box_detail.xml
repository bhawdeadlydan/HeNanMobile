<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:id="@+id/linearLayout28">

        <TextView
            android:layout_width="175dp"
            android:layout_height="wrap_content"
            android:text="是否套设备："
            android:id="@+id/text_delivery_scan_box_detail_is_bom" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout28"
        android:paddingLeft="20dp"
        android:id="@+id/linearLayout29">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="物料名称："
            android:id="@+id/text_delivery_scan_box_detail_item_name" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout29"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="20dp"
        android:id="@+id/linearLayout19">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="物料编码："
            android:id="@+id/text_delivery_scan_box_detail_item_code" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout19"
        android:paddingLeft="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="数量："
            android:id="@+id/text_delivery_scan_box_detail_quantity" />
    </LinearLayout>

</RelativeLayout>
