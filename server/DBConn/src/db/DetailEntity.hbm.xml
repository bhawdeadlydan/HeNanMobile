<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.DetailEntity" table="detail" schema="mobile">
        <id name="id" column="ID"/>
        <property name="isBom" column="IS_BOM"/>
        <property name="saleBomCode" column="SALE_BOM_CODE"/>
        <property name="itemErpCode" column="ITEM_ERP_CODE"/>
        <property name="itemErpUnit" column="ITEM_ERP_UNIT"/>
        <property name="itemCode" column="ITEM_CODE"/>
        <property name="itemName" column="ITEM_NAME"/>
        <property name="itemUnitCode" column="ITEM_UNIT_CODE"/>
        <property name="expectedQuantity" column="EXPECTED_QUANTITY"/>
        <property name="posApplyDocCode" column="POS_APPLY_DOC_CODE"/>
    </class>
</hibernate-mapping>