<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.StagingsiteEntity" table="stagingsite" schema="mobile">
        <id name="id" column="ID"/>
        <property name="applyPerson" column="ApplyPerson"/>
        <property name="constructPos" column="ConstructPos"/>
        <property name="constructUnit" column="ConstructUnit"/>
        <property name="time" column="Time"/>
        <property name="materialCode" column="MaterialCode"/>
        <property name="num" column="Num"/>
    </class>
</hibernate-mapping>